dcmd_unban(playerid, params[])
{
    if(PlayerInfo[playerid][pAdmin] < 5)
	    return 0;

	new rcmsg[80], acct[24], file[64],name[24],string[128],otherfile[64];
	GetPlayerName(playerid, name, sizeof(name));

	if(sscanf(params, "s", acct))
		return SendClientMessage(playerid, COLOR_LIGHTRED, "用法: /unban <账户名称>");

	format(file, sizeof(file),"LARP/Users/%s.ini", acct);

	if(!dini_Exists(file))
	    return SendClientMessage(playerid, COLOR_LIGHTRED,"错误：帐户不存在！");

	dini_IntSet(file, "Banned", 0);

	format(ipFILE, 128, "LARP/Users/Aliases/%s.txt", acct);

	format(rcmsg, sizeof(rcmsg), "unbanip %s", dini_Get(ipFILE,"NameIp"));
	SendRconCommand(rcmsg);
	SendRconCommand("reloadbans");
	
	format(otherfile,sizeof(otherfile),"LARP/Users/Bans/%s.ban",dini_Get(ipFILE,"NameIp"));
	fremove(otherfile);

	format(string, sizeof(string), "[管理]: %s 取消了对账户名为 %s.", name,acct);
	ABroadCast(COLOR_LIGHTRED,string, 5);
	format(string, sizeof(string), "[管理]: IP %s 已从从禁令名单消除由管理员 %s 消除的.", dini_Get(ipFILE,"NameIp"), name);
	ABroadCast(COLOR_WHITE,string, 5);
	format(string, sizeof(string), "* 管理员 %s 取消了对账户名为 %s 玩家{0FF0FF}解封", name,acct);
	SendClientMessageToAll(COLOR_NICERED,string);
	format(string, sizeof(string), "* 成功解封 %s 已从从禁令名单消除 %s", name,dini_Get(ipFILE,"NameIp"));
	SendClientMessage(playerid,TEAM_GROVE_COLOR,string);

	new y, m, d;
	new h,mi,s;
	getdate(y,m,d);
	gettime(h,mi,s);
	format(string,sizeof(string), "(%d/%d/%d)[%d:%d:%d] %s Unbanned the Account of %s (%s)",d,m,y,h,mi,s,name,acct,dini_Get(ipFILE,"NameIp"));
	LoginLog(string);
	return 1;
}

