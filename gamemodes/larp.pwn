public BackupClear(playerid, calledbytimer)
{
	if(IsPlayerConnected(playerid))
	{
		if(IsACop(playerid) || IsAFreecop(playerid))
		{
			if (PlayerInfo[playerid][pRequestingBackup] == 1)
			{
				for(new i = 0; i < MAX_PLAYERS; i++)
				{
					if(IsPlayerConnected(i))
					{
					    if(PlayerInfo[i][pDuty] == 1)
					    {
							if(IsACop(i) || IsAFreecop(i))
							{
								SetPlayerMarkerForPlayer(i, playerid, TEAM_HIT_COLOR);
							}
						}
					}
				}
				if (calledbytimer != 1)
				{
					SendClientMessage(playerid, TEAM_BLUE_COLOR, "你的备份要求已被清除.");
				}
				else
				{
					SendClientMessage(playerid, TEAM_BLUE_COLOR, "你的备份要求已自动清除.");
				}
				PlayerInfo[playerid][pRequestingBackup] = 0;
			}
			else
			{
				if (calledbytimer != 1)
				{
					SendClientMessage(playerid, COLOR_DARKNICERED, "您没有一个活跃备用请求!");
				}
			}
		}
		else
		{
			if (calledbytimer != 1)
			{
				SendClientMessage(playerid, COLOR_GREY, "你不是警察!");
			}
		}
	}
	return 1;
}

