public ClockSeconds()
{
	new string[24];
	
	if(SecondsTD != 60)
	{
		SecondsTD ++;
	}
	else
	{
	    SecondsTD = 0;
	    MinutesTD ++;
	}

	if(MinutesTD == 60)
	{
	    MinutesTD = 0;
	    HoursTD ++;
	}
	if(HoursTD > 23)
	{
		HoursTD = 0;
	}
