					format(string, sizeof(string), "[PETER]: %s 过度使用驾车杀人,玩家已经被警告.", name2);
					SendAdminMessage(COLOR_YELLOW, string);
					format(string,sizeof(string), "(%d/%d/%d)[%d:%d:%d] Warned by 反外挂检测 - Reason: Driver drive-by kill.",d,m,y,h,mi,s);
					AddPunishment(killerid, string);
				}
			}
		}
